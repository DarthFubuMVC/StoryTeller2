<?xml version="1.0" encoding="utf-8"?>
<CommandLineApplicationReport xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <ApplicationName>dotnet run --</ApplicationName>
  <Commands>
    <CommandReport>
      <Name>agent</Name>
      <Description>Used by dotnet storyteller to remote control the Storyteller specification engine</Description>
      <Arguments>
        <ArgumentReport>
          <Name>port</Name>
          <Description>Port</Description>
        </ArgumentReport>
      </Arguments>
      <Flags>
        <FlagReport>
          <UsageDescription>[-s, --specs &lt;specs&gt;]</UsageDescription>
          <Description>Optional. Override the spec directory</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-p, --path &lt;path&gt;]</UsageDescription>
          <Description>Optional, override the root directory of the project</Description>
        </FlagReport>
      </Flags>
      <Usages>
        <UsageReport>
          <Description>Used by dotnet storyteller to remote control the Storyteller specification engine</Description>
          <Usage>dotnet run -- agent &lt;port&gt; [-s, --specs &lt;specs&gt;] [-p, --path &lt;path&gt;]</Usage>
        </UsageReport>
      </Usages>
    </CommandReport>
    <CommandReport>
      <Name>test</Name>
      <Description>Try to start and warmup the system under test for diagnostics</Description>
      <Arguments />
      <Flags>
        <FlagReport>
          <UsageDescription>[-s, --specs &lt;specs&gt;]</UsageDescription>
          <Description>Optional. Override the spec directory</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-p, --path &lt;path&gt;]</UsageDescription>
          <Description>Optional, override the root directory of the project</Description>
        </FlagReport>
      </Flags>
      <Usages>
        <UsageReport>
          <Description>Try to start and warmup the system under test for diagnostics</Description>
          <Usage>dotnet run -- test [-s, --specs &lt;specs&gt;] [-p, --path &lt;path&gt;]</Usage>
        </UsageReport>
      </Usages>
    </CommandReport>
    <CommandReport>
      <Name>run</Name>
      <Description>Executes Specifications and Writes Results</Description>
      <Arguments>
        <ArgumentReport>
          <Name>specificationorsuite</Name>
          <Description>If specified, only executes a single suite or specification</Description>
        </ArgumentReport>
      </Arguments>
      <Flags>
        <FlagReport>
          <UsageDescription>[-r, --results-path &lt;resultspath&gt;]</UsageDescription>
          <Description>Path to write out the results. Default is stresults.htm</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-l, --lifecycle Acceptance|Regression|Any]</UsageDescription>
          <Description>Optional. Only runs tests with desired lifecyle</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-t, --tracing appveyor|teamcity|verbose|none]</UsageDescription>
          <Description>Choose</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-t, --timeout &lt;timeout&gt;]</UsageDescription>
          <Description>Global timeout for this command in minutes. Defaults to 10</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-o, --open]</UsageDescription>
          <Description>Open the results in a browser after the run. DO NOT DO THIS IN CI!</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-c, --csv &lt;csv&gt;]</UsageDescription>
          <Description>WriteToText the performance data in CSV format to the specified path</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-j, --json &lt;json&gt;]</UsageDescription>
          <Description>WriteToText the raw result information to JSON format at the specified path</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-d, --dump &lt;dump&gt;]</UsageDescription>
          <Description>Dump the raw JSON history of the batch run to the specified path</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-m, --max-attempts &lt;maxattempts&gt;]</UsageDescription>
          <Description>Sets a minimum number of retry attempts for this execution. Default is zero</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-f, --fixtures &lt;fixtures&gt;]</UsageDescription>
          <Description>Optional. Override the fixtures directory</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-s, --specs &lt;specs&gt;]</UsageDescription>
          <Description>Optional. Override the spec directory</Description>
        </FlagReport>
        <FlagReport>
          <UsageDescription>[-p, --path &lt;path&gt;]</UsageDescription>
          <Description>Optional, override the root directory of the project</Description>
        </FlagReport>
      </Flags>
      <Usages>
        <UsageReport>
          <Description>Run all specifications</Description>
          <Usage>dotnet run -- run [-r, --results-path &lt;resultspath&gt;] [-l, --lifecycle Acceptance|Regression|Any] [-t, --tracing appveyor|teamcity|verbose|none] [-t, --timeout &lt;timeout&gt;] [-o, --open] [-c, --csv &lt;csv&gt;] [-j, --json &lt;json&gt;] [-d, --dump &lt;dump&gt;] [-m, --max-attempts &lt;maxattempts&gt;] [-f, --fixtures &lt;fixtures&gt;] [-s, --specs &lt;specs&gt;] [-p, --path &lt;path&gt;]</Usage>
        </UsageReport>
        <UsageReport>
          <Description>Run specified specifications</Description>
          <Usage>dotnet run -- run &lt;specificationorsuite&gt; [-r, --results-path &lt;resultspath&gt;] [-l, --lifecycle Acceptance|Regression|Any] [-t, --tracing appveyor|teamcity|verbose|none] [-t, --timeout &lt;timeout&gt;] [-o, --open] [-c, --csv &lt;csv&gt;] [-j, --json &lt;json&gt;] [-d, --dump &lt;dump&gt;] [-m, --max-attempts &lt;maxattempts&gt;] [-f, --fixtures &lt;fixtures&gt;] [-s, --specs &lt;specs&gt;] [-p, --path &lt;path&gt;]</Usage>
        </UsageReport>
      </Usages>
    </CommandReport>
  </Commands>
</CommandLineApplicationReport>
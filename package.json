    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "body-parser": "^1.14.2",
    "chai": "^3.4.1",
    "eslint": "^3.8.1",
    "express": "^4.13.3",
    "fs-extra": "^0.26.3",
    "growl": "^1.8.1",
    "jsx-loader": "^0.13.2",
